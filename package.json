{
  "name": "grimelist",
  "version": "0.0.0",
  "private": true,
  "scripts": {
    "start": "node ./bin/www",
    "clean": "rm -rf public",
    "build:styles": "lessc src/style/style.less public/style.css",
    "build:mkdir": "mkdir -p public",
    "build:copy": "cp -r static/tracks public && cp -r static/html/. public && cp -r src/js public/js",
    "build": "npm run clean && npm run build:mkdir && npm run build:copy && npm run build:styles"
  },
  "dependencies": {
    "body-parser": "~1.8.1",
    "busboy": "~0.2.9",
    "connect-busboy": "~0.0.2",
    "debug": "~2.0.0",
    "express": "^4.9.7",
    "grunt": "^0.4.5",
    "grunt-autoprefixer": "^1.0.1",
    "grunt-contrib-clean": "^0.6.0",
    "grunt-contrib-copy": "^0.7.0",
    "grunt-contrib-less": "^0.12.0",
    "grunt-contrib-uglify": "^0.6.0",
    "grunt-contrib-watch": "^0.6.1",
    "grunt-express-server": "^0.4.19",
    "grunt-shell": "^1.1.1",
    "jade": "~1.6.0",
    "jquery": "~2.1.3",
    "less-middleware": "1.0.x",
    "letsencrypt-express": "^2.0.2",
    "load-grunt-tasks": "^1.0.0",
    "morgan": "~1.3.0",
    "request": "^2.58.0",
    "serve-favicon": "~2.1.3",
    "uuid": "^2.0.1"
  },
  "devDependencies": {
    "less": "^4.1.3"
  }
}
